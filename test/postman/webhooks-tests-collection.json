                  "",
                  "pm.test('Response is array of webhooks', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('subscriptions');\n    pm.expect(jsonData.subscriptions).to.be.an('array')",
