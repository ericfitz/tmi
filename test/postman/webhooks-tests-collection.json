      "name": "Webhook Subscriptions",
      "item": [
        {
          "name": "Create Webhook Subscription - Success (201)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test Webhook {{$randomInt}}\",\n  \"description\": \"Webhook for API testing\",\n  \"url\": \"https://webhook.site/test-{{$randomUUID}}\",\n  \"events\": [\"threat_model.created\", \"threat_model.updated\"],\n  \"active\": true\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/webhooks/subscriptions",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "webhooks",
                "subscriptions"
              ]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 201', function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test('Response has webhook data', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.id).to.exist;",
                  "    pm.expect(jsonData.name).to.exist;",
                  "    pm.expect(jsonData.url).to.exist;",
                  "    pm.expect(jsonData.events).to.be.an('array');",
                  "    pm.expect(jsonData.active).to.be.true;",
                  "    ",
                  "    pm.collectionVariables.set('webhookId', jsonData.id);",
                  "});",
                  "",
                  "pm.test('Webhook has valid UUID', function () {",
                  "    const jsonData = pm.response.json();",
                  "    const uuidRegex = /^[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$/i;",
                  "    pm.expect(jsonData.id).to.match(uuidRegex);",
                  "});",
                  "",
                  "pm.test('Events are correctly stored', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.events).to.include('threat_model.created');",
                  "    pm.expect(jsonData.events).to.include('threat_model.updated');",
                  "});",
                  "",
                  "pm.test('Response time is acceptable', function () {",
                  "    pm.expect(pm.response.responseTime).to.be.below(1000);",
                  "});"
                ]
              }
            }
          ]
        },
