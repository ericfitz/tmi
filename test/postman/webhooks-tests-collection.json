                  "pm.test('Webhooks have required fields', function () {",
                  "    const jsonData = pm.response.json();",
                  "    if (jsonData.subscriptions.length > 0) {",
                  "        pm.expect(jsonData.subscriptions[0]).to.have.property('id');",
                  "        pm.expect(jsonData.subscriptions[0]).to.have.property('name');",
                  "        pm.expect(jsonData.subscriptions[0]).to.have.property('url');",
                  "        pm.expect(jsonData.subscriptions[0]).to.have.property('events');",
                  "        pm.expect(jsonData.subscriptions[0]).to.have.property('active');",
                  "    }",
                  "});",
                  "",
                  "pm.test('Response time is acceptable', function () {",
