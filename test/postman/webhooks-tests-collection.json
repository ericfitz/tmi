                  "pm.test('Contains created webhook', function () {",
                  "    const jsonData = pm.response.json();",
                  "    const webhookId = pm.collectionVariables.get('webhookId');",
                  "    const found = jsonData.subscriptions.find(wh => wh.id === webhookId);",
                  "    pm.expect(found).to.exist;",
                  "});",
