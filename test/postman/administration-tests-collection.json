      "script": {
        "type": "text/javascript",
        "exec": [
          "// Use pre-authenticated token from environment (admin user)",
          "const adminToken = pm.environment.get('token_admin') || pm.environment.get('token_alice');",
          "if (adminToken) {",
          "    pm.collectionVariables.set('accessToken', adminToken);",
          "    console.log('Using pre-authenticated admin token');",
          "} else {",
          "    console.error('No pre-authenticated admin token found');",
          "}"
