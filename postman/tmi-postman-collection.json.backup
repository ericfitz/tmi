      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// OAuth 2.0 Authentication for TMI API",
          "// Works with oauth-client-callback-stub.py running on port 8079",
          "",
          "const TMI_URL = pm.variables.get('baseUrl') || 'http://127.0.0.1:8080';",
          "const STUB_URL = pm.variables.get('oauthStubUrl') || 'http://127.0.0.1:8079';",
          "const USER = pm.variables.get('loginHint') || 'postman-user';",
          "",
          "console.log(`Authenticating as: ${USER}`);",
          "",
          "// Check if we have a valid token",
          "function isTokenValid(token) {",
          "    if (!token) return false;",
          "    try {",
          "        const parts = token.split('.');",
          "        if (parts.length !== 3) return false;",
          "        const payload = JSON.parse(Buffer.from(parts[1], 'base64').toString());",
          "        const now = Math.floor(Date.now() / 1000);",
          "        return payload.exp && now < payload.exp - 60;",
          "    } catch (e) {",
          "        return false;",
          "    }",
          "}",
          "",
          "const currentToken = pm.collectionVariables.get('access_token');",
          "if (isTokenValid(currentToken)) {",
          "    console.log('Using cached token');",
          "    return;",
          "}",
          "",
          "console.log('Getting new token...');",
          "",
          "// Step 1: Trigger OAuth flow",
          "// The TMI server will redirect to the stub with tokens in query params",
          "pm.sendRequest({",
          "    url: `${TMI_URL}/oauth2/authorize?idp=test&login_hint=${USER}&client_callback=${STUB_URL}/&scope=openid`,",
          "    method: 'GET'",
          "}, function (err, response) {",
          "    if (err) {",
          "        console.error('OAuth initiation failed:', err);",
          "        return;",
          "    }",
          "    ",
          "    // Step 2: Wait for redirect to complete",
          "    setTimeout(function() {",
          "        // Step 3: Get credentials from stub",
          "        pm.sendRequest({",
          "            url: `${STUB_URL}/creds?userid=${USER}`,",
          "            method: 'GET'",
          "        }, function (err, response) {",
          "            if (err || response.code === 404) {",
          "                console.error('Failed to get credentials');",
          "                return;",
          "            }",
          "            ",
          "            const creds = response.json();",
          "            if (creds.access_token) {",
          "                pm.collectionVariables.set('access_token', creds.access_token);",
          "                console.log('✓ Token obtained');",
          "            }",
          "        });",
          "    }, 2000);",
          "});"
        ]
      }
