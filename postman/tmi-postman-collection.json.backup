      "item": [
        {
          "name": "Get API information",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Response status code is 200\", function () {",
                  "    pm.expect(pm.response.code).to.equal(200);",
                  "});",
                  "",
                  "",
                  "pm.test(\"Response has required fields\", function () {",
                  "    const responseData = pm.response.json();",
                  "    ",
                  "    pm.expect(responseData).to.be.an('object');",
                  "    pm.expect(responseData.api).to.exist;",
                  "    pm.expect(responseData.operator).to.exist;",
                  "    pm.expect(responseData.service).to.exist;",
                  "    pm.expect(responseData.status).to.exist;",
                  "",
                  "    pm.expect(responseData.api).to.have.all.keys('specification', 'version');",
                  "    pm.expect(responseData.operator).to.have.all.keys('contact', 'name');",
                  "    pm.expect(responseData.service).to.have.all.keys('build', 'name');",
                  "    pm.expect(responseData.status).to.have.all.keys('code', 'time');",
                  "});",
                  "",
                  "",
                  "pm.test(\"Status code must be a non-empty string\", function () {",
                  "    const responseData = pm.response.json();",
                  "    ",
                  "    pm.expect(responseData).to.be.an('object');",
                  "    pm.expect(responseData.status).to.exist;",
                  "    pm.expect(responseData.status.code).to.be.a('string').and.to.have.lengthOf.above(0, \"Status code should not be empty\");",
                  "});",
                  "",
                  "",
                  "pm.test(\"Version follows semantic versioning format\", function () {",
                  "    const responseData = pm.response.json();",
                  "    ",
                  "    pm.expect(responseData).to.be.an('object');",
                  "    pm.expect(responseData.api).to.have.property('version');",
                  "    pm.expect(responseData.api.version).to.match(/^\\d+\\.\\d+\\.\\d+$/, \"Version format is invalid\");",
                  "});",
                  "",
                  "",
                  "pm.test(\"Response time is less than 200ms\", function () {",
                  "    pm.expect(pm.response.responseTime).to.be.below(200);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
